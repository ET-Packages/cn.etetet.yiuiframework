.container {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.container > .container-item {
    width: 50px;
    height: 50px;
    margin: 5px;
    border-width: 1px;
    border-color: rgb(32, 32, 32);
    border-radius: 5px;
    -unity-background-scale-mode: scale-to-fit;
    cursor: link;
}